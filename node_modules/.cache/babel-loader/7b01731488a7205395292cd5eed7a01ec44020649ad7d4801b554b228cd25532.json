elect=\"setGeneratorSelectedOptions('background', e)\" />\n      <StandardDropdown :disabled=\"!dropdownOptions.value.lineages.length\"\n                        placeholderText=\"Lineages\"\n                        :options=\"lineages\"\n                        selectId=\"char-lineage-dropdown\"\n                        @select=\"setGeneratorSelectedOptions('lineage', e)\"\n                        class=\"lineage-dropdown\" />\n      <StandardDropdown :disabled=\"!dropdownOptions.value.characterSubclasses?.length\"\n                        placeholderText=\"Subclass\"\n                        :options=\"subclasses\"\n                        selectId=\"char-subclass-dropdown\"\n                        @select=\"setGeneratorSelectedOptions('sSubclass', e)\"\n                        class=\"subclasses-dropdown\" />\n      <StandardDropdown :disabled=\"!dropdownOptions.value.appearances?.length\"\n                        placeholderText=\"Appearance\"\n                        :options=\"dropdownOptions.value.appearances\"\n                        selectId=\"char-appearances-dropdown\"\n                        @select=\"setGeneratorSelectedOptions('appearance', e)\"\n                        class=\"appearances-dropdown\" />\n    </div>\n    <div class=\"footer\">\n      <StandardButton @click=\"console.log(gameSystemName)\">\n        <img src=\"@/assets/d100.png\" alt=\"Roll a character\" />\n      </StandardButton>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { inject, ref, watch } from 'vue'\n\nimport StandardButton from \"../StandardComponents/StandardButton/StandardButton\";\nimport StandardDropdown from \"../StandardComponents/StandardDropdown/StandardDropdown\";\n\nconst props = defineProps({\n  loading: Boolean\n});\n\nwatch(loading, (wasLoading, isLoading) => {\n  if (!isLoading) {\n    const { \n    gameSystemName,\n    dropdownOptions,\n    fetchLineages,\n    fetchSubclasses\n  } = inject('generatorData');\n\n  const { setGeneratorSelectedOptions } = inject('generatorSelected');\n  }\n})\n\n\n\nconst lineages = ref([]);\nconst subclasses = ref([]);\n\nfunction selectAncestry (ancestry) {\n  setGeneratorSelectedOptions('ancestry', ancestry);\n  fetchLineages(ancestry, (newLineages) => { lineages.value = newLineages });\n}\n\nfunction selectClass (playerClass) {\n  setGeneratorSelectedOptions('class', playerClass);\n  fetchSubclasses(playerClass, (newSubclasses) => { subclasses.value = newSubclasses })\n}\n</script>\n\n<style scoped lang=\"scss\">\n@import \"./CharacterGeneratorRoller.scss\";\n</style>"],"mappings":"AA2DA,SAASA,MAAM,EAAEC,GAAG,EAAEC,KAAK,QAAQ,KAAK;AAExC,OAAOC,cAAc,MAAM,qDAAqD;AAChF,OAAOC,gBAAgB,MAAM,yDAAyD;;;;;;;;;;IAEtF,MAAMC,KAAK,GAAGC,OAEZ;IAEFJ,KAAK,CAACK,OAAO,EAAE,CAACC,UAAU,EAAEC,SAAS,KAAK;MACxC,IAAI,CAACA,SAAS,EAAE;QACd,MAAM;UACNC,cAAc;UACdC,eAAe;UACfC,aAAa;UACbC;QACF,CAAC,GAAGb,MAAM,CAAC,eAAe,CAAC;QAE3B,MAAM;UAAEc;QAA4B,CAAC,GAAGd,MAAM,CAAC,mBAAmB,CAAC;MACnE;IACF,CAAC,CAAC;IAIF,MAAMe,QAAQ,GAAGd,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMe,UAAU,GAAGf,GAAG,CAAC,EAAE,CAAC;IAE1B,SAASgB,cAAcA,CAAEC,QAAQ,EAAE;MACjCJ,2BAA2B,CAAC,UAAU,EAAEI,QAAQ,CAAC;MACjDN,aAAa,CAACM,QAAQ,EAAGC,WAAW,IAAK;QAAEJ,QAAQ,CAACK,KAAK,GAAGD,WAAW;MAAC,CAAC,CAAC;IAC5E;IAEA,SAASE,WAAWA,CAAEC,WAAW,EAAE;MACjCR,2BAA2B,CAAC,OAAO,EAAEQ,WAAW,CAAC;MACjDT,eAAe,CAACS,WAAW,EAAGC,aAAa,IAAK;QAAEP,UAAU,CAACI,KAAK,GAAGG,aAAa;MAAC,CAAC,CAAC;IACvF"},"metadata":{},"sourceType":"module","externalDependencies":[]}