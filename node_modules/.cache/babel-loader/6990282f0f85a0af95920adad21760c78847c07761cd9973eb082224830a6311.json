{"ast":null,"code":"import { inject, ref } from 'vue';\nimport StandardTextbox from \"../StandardComponents/StandardTextbox/StandardTextbox\";\nimport StandardDropdown from \"../StandardComponents/StandardDropdown/StandardDropdown\";\nexport default {\n  __name: 'CharacterCardNew',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const {\n      dropdownOptions,\n      fetchLineages,\n      fetchSubclasses\n    } = inject('generatorData');\n    const {\n      generatorSelectedOptions\n    } = inject('generatorData');\n    const lineages = ref([]);\n    const subclasses = ref([]);\n    function selectAncestry(ancestry) {\n      fetchLineages(ancestry, newLineages => {\n        lineages.value = newLineages;\n      });\n    }\n    function selectClass(playerClass) {\n      fetchSubclasses(playerClass, newSubclasses => {\n        subclasses.value = newSubclasses;\n      });\n    }\n    const __returned__ = {\n      dropdownOptions,\n      fetchLineages,\n      fetchSubclasses,\n      generatorSelectedOptions,\n      lineages,\n      subclasses,\n      selectAncestry,\n      selectClass,\n      inject,\n      ref,\n      get StandardTextbox() {\n        return StandardTextbox;\n      },\n      get StandardDropdown() {\n        return StandardDropdown;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["inject","ref","StandardTextbox","StandardDropdown","dropdownOptions","fetchLineages","fetchSubclasses","generatorSelectedOptions","lineages","subclasses","selectAncestry","ancestry","newLineages","value","selectClass","playerClass","newSubclasses"],"sources":["/Users/lappy486/Code/pwf-games/src/components/Cards/CharacterCardNew.vue"],"sourcesContent":["<template>\n  <div class=\"character-card\">\n    <StandardTextbox placeholder=\"Name\"\n                     id=\"char-name\"\n                     :defaultV