e\"\n                      :options=\"dropdownOptions.appearances\"\n                      selectId=\"char-appearance-dropdown\"\n                      :defaultSelectedValue=\"generatorSelectedOptions.value.appearance\"\n                      class=\"character-appearance\" />\n  </div>\n</template>\n\n<script setup>\nimport { inject, ref } from 'vue'\n\nimport StandardTextbox from \"../StandardComponents/StandardTextbox/StandardTextbox\";\nimport StandardDropdown from \"../StandardComponents/StandardDropdown/StandardDropdown\";\n\nconst { \n  dropdownOptions,\n  fetchLineages,\n  fetchSubclasses\n} = inject('generatorData');\n\nconst { generatorSelectedOptions } = inject('generatorData');\n\nconst lineages = ref([]);\nconst subclasses = ref([]);\n\nfunction selectAncestry (ancestry) {\n  fetchLineages(ancestry, (newLineages) => { lineages.value = newLineages });\n}\n\nfunction selectClass (playerClass) {\n  fetchSubclasses(playerClass, (newSubclasses) => { subclasses.value = newSubclasses })\n}\n</script>\n\n<style scoped lang=\"scss\">\n@import \"./CharacterCard.scss\";\n</style>"],"mappings":"AA0CA,SAASA,MAAM,EAAEC,GAAG,QAAQ,KAAK;AAEjC,OAAOC,eAAe,MAAM,uDAAuD;AACnF,OAAOC,gBAAgB,MAAM,yDAAyD;;;;;;;IAEtF,MAAM;MACJC,eAAe;MACfC,aAAa;MACbC;IACF,CAAC,GAAGN,MAAM,CAAC,eAAe,CAAC;IAE3B,MAAM;MAAEO;IAAyB,CAAC,GAAGP,MAAM,CAAC,eAAe,CAAC;IAE5D,MAAMQ,QAAQ,GAAGP,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMQ,UAAU,GAAGR,GAAG,CAAC,EAAE,CAAC;IAE1B,SAASS,cAAcA,CAAEC,QAAQ,EAAE;MACjCN,aAAa,CAACM,QAAQ,EAAGC,WAAW,IAAK;QAAEJ,QAAQ,CAACK,KAAK,GAAGD,WAAW;MAAC,CAAC,CAAC;IAC5E;IAEA,SAASE,WAAWA,CAAEC,WAAW,EAAE;MACjCT,eAAe,CAACS,WAAW,EAAGC,aAAa,IAAK;QAAEP,UAAU,CAACI,KAAK,GAAGG,aAAa;MAAC,CAAC,CAAC;IACvF"},"metadata":{},"sourceType":"module","externalDependencies":[]}